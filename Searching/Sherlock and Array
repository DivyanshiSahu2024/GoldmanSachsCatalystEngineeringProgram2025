
public static String balancedSums(List<Integer> arr) {

  int totalSum = 0;

  for (int num : arr) {

    totalSum += num;

  }

  int leftSum = 0;

  for (int i = 0; i < arr.size(); i++) {

    int current = arr.get(i);

    int rightSum = totalSum - leftSum - current;

    if (leftSum == rightSum) {

      return "YES";

    }

    leftSum += current;

  }

  return "NO";

}

